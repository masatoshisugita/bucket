<div id="bbb" class="main-area">
<div class="content">
    <h1>ユーザー登録</h1>
  <div class="main-content">
      <%= form_tag ("/users/create") do  %>
      <div class="form">
        <p>名前</p>
        <input type="text" name = "name">
      </div>
      <div class="form">
        <p>パスワード</p>
        <input name = "password" type="password">
        <input type ="submit" value="登録">
      </div>
    <% end %>
  </div>
</div>
</div>
